{
  "name": "avbot",
  "version": "4.0.0",
  "description": "AvBot v4",
  "author": "Rahul Singh <hey.singh@protonmail.com>",
  "private": true,
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "serve": "node build/main.js",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "lint-fix": "eslint . --ext .ts --fix",
    "prettier": "prettier . -w",
    "start_js": "npm run build & node build/main.js",
    "restart_p2": "pm2 restart ../ecosystem/ecosystem.config.js --only avbot",
    "restart_p2_debug": "pm2 restart ../ecosystem/ecosystem.config.js --only avbot -- -debug",
    "start_p2": "pm2 start ../ecosystem/ecosystem.config.js --only avbot",
    "start_p2_debug": "pm2 start ../ecosystem/ecosystem.config.js --only avbot -- -debug",
    "build_and_start": "npm run build & npm run restart_p2",
    "build_and_start_debug": "npm run build & npm run restart_p2_debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drph4nt0m/avbot-v3.git"
  },
  "bugs": {
    "url": "https://github.com/drph4nt0m/avbot-v3/issues"
  },
  "homepage": "https://github.com/drph4nt0m/avbot-v3#readme",
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.9.0",
    "@discordx/importer": "^1.1.9",
    "@discordx/pagination": "^2.1.2",
    "@discordx/utilities": "4.2.5",
    "@overnightjs/core": "^1.7.6",
    "@types/common-tags": "^1.8.1",
    "axios": "^0.27.2",
    "common-tags": "^1.8.2",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "datadog-metrics": "^0.9.3",
    "dayjs": "^1.10.7",
    "discord-api-types": "^0.32.1",
    "discord.js": "^13.7.0",
    "discordx": "^9.6.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "ffmpeg-static": "^5.0.0",
    "http-status-codes": "^2.2.0",
    "immutable": "^4.0.0",
    "libsodium-wrappers": "^0.7.10",
    "mongodb": "^4.1.3",
    "node-gtts": "^2.0.2",
    "opusscript": "0.0.8",
    "reflect-metadata": "^0.1.13",
    "remove-accents": "^0.4.2",
    "tmp": "^0.2.1",
    "toad-scheduler": "^1.6.0",
    "tsyringe": "^4.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
